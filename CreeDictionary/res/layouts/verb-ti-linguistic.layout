name: "Transitive inanimate verb full (linguistic) paradigm"
paradigm: "verb-ti.paradigm"
layout:
  type: "linguistic"
tooltips:
  eng:
    "1s": "1st person singular actor"
    "2s": "2nd person singular actor"
    "3s": "3rd person singular actor"
    "1p": "1st person plural exclusive actor"
    "21": "1st person plural inclusive actor"
    "2p": "2nd person plural actor"
    "3p": "3rd person plural actor"
    "4": "3rd person obviative actor"
    "X": "Unspecified actor"
